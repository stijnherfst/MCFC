//===========================================================================
// Trigger: Add Strength
//===========================================================================
function Add_Strength_UnitTypeBool takes nothing returns boolean
    return IsUnitType(GetFilterUnit(), UNIT_TYPE_HERO)
endfunction

function Trig_Add_Strength_Actions takes nothing returns nothing
    local integer currentLumber= GetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_LUMBER)
    local integer statIncrease= S2I(SubString(GetEventPlayerChatString(), 5, StringLength(GetEventPlayerChatString())))
    local boolexpr b= Condition(function Add_Strength_UnitTypeBool)
    local group g= CreateGroup()
    local unit u
    
    if statIncrease <= currentLumber and statIncrease > 0 then
        call SetPlayerState(GetTriggerPlayer(), PLAYER_STATE_RESOURCE_LUMBER, currentLumber - statIncrease)
        
        call GroupEnumUnitsOfPlayer(g, GetTriggerPlayer(), b)
        set u=FirstOfGroup(g)
        
        call SetHeroStr(u, GetHeroStr(u, false) + statIncrease, true)
    endif
    
    call DestroyGroup(g)
    call DestroyBoolExpr(b)
    set u=null
    set g=null
    set b=null
endfunction

