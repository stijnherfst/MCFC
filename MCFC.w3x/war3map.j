function Trig_Escort_Quest_End_Copy_2_Func001001002 takes nothing returns boolean
    return ( IsUnitType(GetFilterUnit(), UNIT_TYPE_HERO) == true )
endfunction

function Trig_Escort_Quest_End_Give_Spell takes nothing returns nothing
    call UnitAddItemByIdSwapped('I0AF', GetEnumUnit())
endfunction

function Trig_Escort_Quest_End_Give_Gold takes nothing returns nothing
    call SetPlayerState(GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_GOLD, GetPlayerState(GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_GOLD) + 10000)
endfunction

function Trig_Escort_Quest_End_Copy_Conditions takes nothing returns boolean
    if ( IsUnitType(GetTriggerUnit(), UNIT_TYPE_HERO) == false ) then
        return false
    elseif ( GetOwningPlayer(GetTriggerUnit()) == Player(10) ) then
        return false
    elseif ( GetOwningPlayer(GetTriggerUnit()) == Player(11) ) then
        return false
    endif
    return true
endfunction

function Trig_Escort_Quest_End_Copy_Actions takes nothing returns nothing
    call DestroyTrigger(GetTriggeringTrigger())
	
	if ( udg_MerchantQuestReward == 4 ) then
		call DisplayTextToForce(GetPlayersAll(), "Thanks mate, we couldn't have made it without ya. Here's a little something for the trouble. If ya ever need to hitch a ride to the brave new world then just hop on our battleship in a new 7.0 beta or something.")
		call ForGroupBJ(GetUnitsInRectMatching(gg_rct_Town_of_Dickus_Bickus, Condition(function Trig_Escort_Quest_End_Copy_2_Func001001002)), function Trig_Escort_Quest_End_Give_Spell)
	elseif ( udg_MerchantQuestReward == 3 ) then
		call DisplayTextToForce(GetPlayersAll(), "We didn't all make it. Not like I was attached to em or anything, but it hurts business. Here's a little something for the trouble.")
		call ForGroupBJ(GetUnitsInRectMatching(gg_rct_Town_of_Dickus_Bickus, Condition(function Trig_Escort_Quest_End_Copy_2_Func001001002)), function Trig_Escort_Quest_End_Give_Spell)
	elseif ( udg_MerchantQuestReward == 2 ) then
		call DisplayTextToForce(GetPlayersAll(), "May their souls rest in peace. It's gonna be a lil harder moving forward, but here's what me can spare.")
		call ForGroupBJ(GetUnitsInRectMatching(gg_rct_Town_of_Dickus_Bickus, Condition(function Trig_Escort_Quest_End_Copy_2_Func001001002)), function Trig_Escort_Quest_End_Give_Gold)
	elseif ( udg_MerchantQuestReward == 1 ) then
		call DisplayTextToForce(GetPlayersAll(), "They're all dead mate. I mean... did you even try? Ya'll be getting nothing from me you shitty adventurers.")
	endif
endfunction

