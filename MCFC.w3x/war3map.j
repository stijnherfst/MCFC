 local trigger l__gg_trg_ChangeTypeByItem= CreateTrigger()
	call TriggerRegisterPlayerUnitEvent(l__gg_trg_ChangeTypeByItem, Player(0), EVENT_PLAYER_UNIT_PICKUP_ITEM, null)
