    local integer i= 0
	loop
	    exitwhen ( i > 9 )
		call TriggerRegisterPlayerChatEvent(l__gg_trg_Add_Strength, Player(i), "-str", false)
	    set i=i + 1
	endloop
	call TriggerAddAction(l__gg_trg_Add_Strength, function Trig_Add_Strength_Actions)
    set l__gg_trg_Add_Strength=null
endfunction
