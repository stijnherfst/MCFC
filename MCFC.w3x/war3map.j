//===========================================================================
// Trigger: ChangeTypeByItem
//===========================================================================
function Trig_ChangeTypeByItem_Conditions takes nothing returns boolean
	return GetItemType(GetManipulatedItem()) == ITEM_TYPE_ARTIFACT
endfunction

function Trig_ChangeTypeByItem_Actions takes nothing returns nothing
 local integer item_type= GetItemTypeId(GetManipulatedItem())
 local integer temp_ability
 local integer i= 0
 local real hp
 local real mp
	
	// If the unit is not a hero then do nothing
	if ( not IsHeroUnitId(GetUnitTypeId(GetTriggerUnit())) ) then
		return
	endif
	
	// If the unit already has a weapon then do nothing
	if ( UnitHasItemOfType(GetTriggerUnit() , ITEM_TYPE_ARTIFACT) >= 2 ) then
		call DisplayTimedTextToPlayer(GetLocalPlayer(), 0, 0, 50, "You already have a weapon equiped")
		return
	endif
	
	// If the unit type the item represents is unknown do nothing
	if ( not (HaveSavedInteger(Table__ht, (item_to_unit_type), (item_type))) ) then // INLINED!!
		call DisplayTimedTextToForce(bj_FORCE_ALL_PLAYERS, 50.00, "Uknown type. Report this to eejin")
		return
	endif
	
	// If of same type family then do nothing (eg. Normal type and Normal Flyer
	if ( (LoadInteger(Table__ht, (type_family), (GetUnitTypeId(GetTriggerUnit())))) == (LoadInteger(Table__ht, (type_family), ((LoadInteger(Table__ht, (item_to_unit_type), (item_type)))))) ) then // INLINED!!
		return
	endif

	set bj_lastCreatedUnit=CreateUnitAtLoc(GetOwningPlayer(GetTriggerUnit()), (LoadInteger(Table__ht, (item_to_unit_type), (item_type))), GetUnitLoc(GetTriggerUnit()), GetUnitFacing(GetTriggerUnit())) // INLINED!!
	
	set hp=GetUnitState(GetTriggerUnit(), UNIT_STATE_LIFE)
	set mp=GetUnitState(GetTriggerUnit(), UNIT_STATE_MANA)

	call SetHeroStr(bj_lastCreatedUnit, GetHeroStr(GetTriggerUnit(), false), true)
	call SetHeroAgi(bj_lastCreatedUnit, GetHeroAgi(GetTriggerUnit(), false), true)
	call SetHeroInt(bj_lastCreatedUnit, GetHeroInt(GetTriggerUnit(), false), true)
	call SetHeroXP(bj_lastCreatedUnit, GetHeroXP(GetTriggerUnit()), false)
	
	call UnitAddItem(bj_lastCreatedUnit, UnitItemInSlot(GetTriggerUnit(), 0))
	call UnitAddItem(bj_lastCreatedUnit, UnitItemInSlot(GetTriggerUnit(), 1))
	call UnitAddItem(bj_lastCreatedUnit, UnitItemInSlot(GetTriggerUnit(), 2))
	call UnitAddItem(bj_lastCreatedUnit, UnitItemInSlot(GetTriggerUnit(), 3))
	call UnitAddItem(bj_lastCreatedUnit, UnitItemInSlot(GetTriggerUnit(), 4))
	call UnitAddItem(bj_lastCreatedUnit, UnitItemInSlot(GetTriggerUnit(), 5))

	call SetUnitState(bj_lastCreatedUnit, UNIT_STATE_LIFE, hp)
	call SetUnitState(bj_lastCreatedUnit, UNIT_STATE_MANA, mp)
	
	call RemoveUnit(GetTriggerUnit())
	
	set i=0
	loop
		exitwhen i > 6
		set temp_ability=abilities[s__playerAbilities[GetPlayerId(GetOwningPlayer(bj_lastCreatedUnit)) * 7 + i]]
		call UnitAddAbility(bj_lastCreatedUnit, temp_ability)
		call SetUnitAbilityLevel(bj_lastCreatedUnit, temp_ability, GetUnitAbilityLevel(GetTriggerUnit(), temp_ability))
		set i=i + 1
	endloop
	
	if ( GetLocalPlayer() == GetOwningPlayer(bj_lastCreatedUnit) ) then
		call ClearSelection()
		call SelectUnit(bj_lastCreatedUnit, true)
	endif
endfunction

