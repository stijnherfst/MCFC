    if ( not ( CountUnitsInGroup(GetUnitsInRectOfPlayer(gg_rct_Volcano_Challenge, Player(11))) == 0 ) ) then
        return false
    endif
    if ( not ( GetUnitTypeId(GetTriggerUnit()) == 'n04G' ) ) then
