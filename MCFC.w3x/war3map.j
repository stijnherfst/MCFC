function Trig_Chain_Create_Actions takes nothing returns nothing
    // DON'T TOUCH ANYTHING HERE
    // 1 is to check how many spells are running. 2 is for storage
    // If empty instances, run trigger, so that when it is above 1, it keep calling it to turn on.
    if ( Trig_Chain_Create_Func004C() ) then
        call EnableTrigger(gg_trg_Chain_Jump)
    else
    endif
    // --------------------------------------------------------------
    // temp Position of caster for other usage
    set udg_QJCS_tempPos[1]=GetUnitLoc(udg_QJC_Caster)
    // --------------------------------------------------------------
    // Limitations
    // Jump Count Limit Check
    if ( Trig_Chain_Create_Func011C() ) then
        set udg_QJC_JumpCount=1
    else
    endif
    // Target Unit Limitations
    if ( Trig_Chain_Create_Func013C() ) then
        // If No Target, get nearest target around caster
        set udg_QJCS_tempGroup=GetUnitsInRangeOfLocMatching(udg_QJC_JumpRadius, udg_QJCS_tempPos[1], Condition(function Trig_Chain_Create_Func013Func003002003))
        set udg_QJC_TargetUnit=GroupPickRandomUnit(udg_QJCS_tempGroup)
        call DestroyGroup(udg_QJCS_tempGroup)
    else
        if ( Trig_Chain_Create_Func013Func001C() ) then
            set udg_QJCS_tempGroup=GetUnitsInRangeOfLocMatching(udg_QJC_JumpRadius, udg_QJC_TargetPoint, Condition(function Trig_Chain_Create_Func013Func001Func001002003))
            set udg_QJC_TargetUnit=GroupPickRandomUnit(udg_QJCS_tempGroup)
            call DestroyGroup(udg_QJCS_tempGroup)
            call RemoveLocation(udg_QJC_TargetPoint)
        else
        endif
    endif
    // if Reduction Limit > 1.00 = 100%
    if ( Trig_Chain_Create_Func015C() ) then
        set udg_QJC_AmountReduce=1.00
    else
    endif
    // Reduction Limit < 0.00 = 0%
    if ( Trig_Chain_Create_Func017C() ) then
        set udg_QJC_AmountReduce=0.00
    else
    endif
    // if Ally and Enemy = False
    if ( Trig_Chain_Create_Func019C() ) then
        set udg_QJC_Enemy=true
    else
    endif
    // Jump Delay Time Max = 1.00
    if ( Trig_Chain_Create_Func021C() ) then
        set udg_QJC_JumpDelayTime=1.00
    else
    endif
    if ( Trig_Chain_Create_Func022C() ) then
        set udg_QJC_JumpDelayTime=0.00
    else
    endif
    // --------------------------------------------------------------
    // Increase Index 1 and 2
    set udg_QJCS_index[1]=( udg_QJCS_index[1] + 1 )
    set udg_QJCS_index[2]=( udg_QJCS_index[2] + 1 )
    // Store User Data to index 2
    // Caster
    set udg_QJCS_Caster[udg_QJCS_index[2]]=udg_QJC_Caster
    // Victim
    set udg_QJCS_TargetUnit[udg_QJCS_index[2]]=udg_QJC_TargetUnit
    // Priority
    set udg_QJCS_Priority[udg_QJCS_index[2]]=udg_QJC_Priority
    // OnePerUnit (OPU)
    set udg_QJCS_OnePerUnit[udg_QJCS_index[2]]=udg_QJC_OnePerUnit
    // Types
    set udg_QJCS_Damage[udg_QJCS_index[2]]=udg_QJC_Damage
    set udg_QJCS_Heal[udg_QJCS_index[2]]=udg_QJC_Heal
    set udg_QJCS_Health[udg_QJCS_index[2]]=udg_QJC_Heatlh
    set udg_QJCS_Mana[udg_QJCS_index[2]]=udg_QJC_Mana
    set udg_QJCS_Gold[udg_QJCS_index[2]]=udg_QJC_Gold
    set udg_QJCS_Leech[udg_QJCS_index[2]]=udg_QJC_Leech
    // Target Types
    set udg_QJCS_Ally[udg_QJCS_index[2]]=udg_QJC_Ally
    set udg_QJCS_Enemy[udg_QJCS_index[2]]=udg_QJC_Enemy
    // Amount of Damage/Heal/Health/Mana/Gold
    set udg_QJCS_Amount[udg_QJCS_index[2]]=udg_QJC_Amount
    // Attack & Damage Type
    set udg_QJCS_AttackType[udg_QJCS_index[2]]=udg_QJC_AttackType
    set udg_QJCS_DamageType[udg_QJCS_index[2]]=udg_QJC_DamageType
    // Damage Reduction
    set udg_QJCS_AmountReduce[udg_QJCS_index[2]]=udg_QJC_AmountReduce
    // Jump Count
    set udg_QJCS_JumpCount[udg_QJCS_index[2]]=udg_QJC_JumpCount
    // Jump Delay
    set udg_QJCS_JumpDelayTime[udg_QJCS_index[2]]=udg_QJC_JumpDelayTime
    // Jump Radius
    set udg_QJCS_JumpRadius[udg_QJCS_index[2]]=udg_QJC_JumpRadius
    // Save AoE
    set udg_QJCS_AoE[udg_QJCS_index[2]]=udg_QJC_AoE
    if ( Trig_Chain_Create_Func061C() ) then
        set udg_QJCS_AoERadius[udg_QJCS_index[2]]=udg_QJC_AoERadius
    else
    endif
    // Save SFX
    set udg_QJCS_ChainSFX[udg_QJCS_index[2]]=udg_QJC_ChainSFX
    set udg_QJCS_TargetSFX[udg_QJCS_index[2]]=udg_QJC_TargetSFX
    // Save Slow
    set udg_QJCS_Slow[udg_QJCS_index[2]]=udg_QJC_Slow
    if ( Trig_Chain_Create_Func067C() ) then
        set udg_QJCS_SlowEffect[udg_QJCS_index[2]]=udg_QJC_SlowEffect
    else
    endif
    // Save Stun
    set udg_QJCS_Stun[udg_QJCS_index[2]]=udg_QJC_Stun
    if ( Trig_Chain_Create_Func070C() ) then
        set udg_QJCS_StunEffect[udg_QJCS_index[2]]=udg_QJC_StunEffect
    else
    endif
    // Save System Time & System Jump Count
    set udg_QJCS_SystemCount[udg_QJCS_index[2]]=0
    set udg_QJCS_SystemTime[udg_QJCS_index[2]]=0.00
    // Create dGroup
    set udg_QJCS_dGroup[udg_QJCS_index[2]]=CreateGroup()
    // Create Base Dummy
    call CreateNUnitsAtLoc(1, 'e001', GetOwningPlayer(udg_QJC_Caster), udg_QJCS_tempPos[1], bj_UNIT_FACING)
    set udg_QJCS_baseDummy[udg_QJCS_index[2]]=GetLastCreatedUnit()
    // Add dummy to group
    call GroupAddUnitSimple(GetLastCreatedUnit(), udg_QJCS_dummyGroup)
    // Clear
    call RemoveLocation(udg_QJCS_tempPos[1])
endfunction

